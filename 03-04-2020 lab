#3
def PrimeChecker():
  import random
  a=int(input("Please enter a number: "))
  if(a<0):
    return "Negative number can't be prime number."
  elif(a>5 and a/a==1 and a/1==a and a%2!=0 and a%3!=0 and a%5!=0):
    return True
  elif(a==2 or a==3 or a==5):
    return True
  return False

PrimeChecker()

#4 Perfect numbers
def divisor(n):
  i=1
  while(i<=n):
    if(n%i==0):
      return i
    i+=1

